{
    "name": "ekline-backend",
    "version": "1.0.0",
    "main": "src/server.ts",
    "scripts": {
        "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
        "start": "node dist/server.js",
        "build": "tsc",
        "migrate": "ts-node --transpile-only -r tsconfig-paths/register scripts/migrate.ts",
        "migrate:up": "ts-node --transpile-only -r tsconfig-paths/register scripts/migrate.ts up",
        "migrate:down": "ts-node --transpile-only -r tsconfig-paths/register scripts/migrate.ts down",
        "migrate:create": "ts-node --transpile-only -r tsconfig-paths/register scripts/migrate.ts create",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "dependencies": {
        "aws-sdk": "^2.1467.0",
        "axios": "^1.7.8",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "crypto": "^1.0.1",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "jsonwebtoken": "^9.0.2",
        "multer": "^2.0.2",
        "pg": "^8.12.0",
        "redis": "^5.10.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/multer": "^2.0.0",
        "@types/node": "^22.1.0",
        "@types/node-pg-migrate": "^2.3.1",
        "@types/pg": "^8.15.6",
        "@types/supertest": "^6.0.3",
        "jest": "^30.2.0",
        "node-pg-migrate": "^8.0.3",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "tsconfig-paths-jest": "^0.0.1",
        "tsx": "^4.21.0",
        "typescript": "^5.5.4"
    }
}
